<h1> Check Out Page </h1>

<%carted_products.each do |carted_product| %>
  <%= carted_product.product.name %> (<%= carted_product.quantity %>)
  <%= link_to "Remove item from cart", "/carted_products/#{carted_product.id}", method: :delete %>  
<% end %>

<%= form_tag "/orders", method: :post do %>
  <%= submit_tage "Buy now!!" %>
<% end %>




